@ARTICLE{Li2002-jq,
  title    = "Induction and suppression of {RNA} silencing by an animal virus",
  author   = "Li, Hongwei and Li, Wan Xiang and Ding, Shou Wei",
  abstract = "RNA silencing is a sequence-specific RNA degradation mechanism
              that is operational in plants and animals. Here, we show that
              flock house virus (FHV) is both an initiator and a target of RNA
              silencing in Drosophila host cells and that FHV infection
              requires suppression of RNA silencing by an FHV-encoded protein,
